<?xml version="1.0" encoding="UTF-8"?>
<metametamodel:Schema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metametamodel="http://www.example.org/metametamodel"
    xsi:schemaLocation="http://www.example.org/metametamodel metametamodel.ecore"
    name="CommunicationSchema">
  <attribute
      name="communicationID"/>
  <dataschema
      name="DataSchema">
    <attribute
        name="connectionID"/>
    <resource
        name="Request">
      <subResources
          name="MediaRequest"/>
      <subResources
          name="FormRequest"/>
    </resource>
    <resource
        name="DataContent">
      <subResources
          name="Medium"/>
      <subResources
          name="Form">
        <composition
            sourceCardinality="ONE"
            targetCardinality="ONE_OR_ANY"
            target="//@dataschema/@resource.1/@subResources.0"/>
        <composition
            sourceCardinality="ONE"
            targetCardinality="ONE_OR_ANY"
            target="//@dataschema/@resource.1/@subResources.1"/>
      </subResources>
    </resource>
  </dataschema>
</metametamodel:Schema>
