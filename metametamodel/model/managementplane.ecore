<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="managementplane" nsURI="managementplane" nsPrefix="http://www.example.org/managementplane">
  <eClassifiers xsi:type="ecore:EClass" name="Signal" eSuperTypes="common.ecore#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="ecore:EClass common.ecore#//DomainElement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ManagementPlane" eSuperTypes="metametamodel.ecore#//Plane">
    <eStructuralFeatures xsi:type="ecore:EReference" name="signal" upperBound="-1"
        eType="#//Signal" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" upperBound="-1"
        eType="#//Condition" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="action" upperBound="-1"
        eType="#//Action" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Condition" eSuperTypes="common.ecore#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="trigger" lowerBound="1"
        upperBound="-1" eType="#//Action"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="catch" upperBound="-1"
        eType="#//Signal"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="expression" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Action" eSuperTypes="common.ecore#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" upperBound="-1"
        eType="ecore:EClass common.ecore#//DomainElement"/>
  </eClassifiers>
</ecore:EPackage>
