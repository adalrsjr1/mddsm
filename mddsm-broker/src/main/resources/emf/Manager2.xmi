<?xml version="1.0" encoding="ASCII"?>
<sb.base:Manager
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sb.base="http://www.inf.ufg.br/mestrado/sb/base"
    xsi:schemaLocation="http://www.inf.ufg.br/mestrado/sb/base base.ecore"
    name="LC">
  <resourceManager
      xsi:type="sb.base:InstanceResourceManager"
      Name="LC">
    <instances
        name="LampDevice">
      <features
          name="Load">
        <attributes
            name="Name"/>
        <attributes
            name="Id"/>
        <attributes
            name="Wattage"
            value="25"/>
        <attributes
            name="Operational"
            value="true"/>
        <attributes
            name="Properties"/>
        <attributes
            name="Control"
            value="Controllable"/>
        <attributes
            name="Critical"
            value="Sensitive"/>
        <attributes
            name="PlantE_ID"/>
      </features>
    </instances>
    <instances
        name="Src">
      <features
          name="GeneratorSource">
        <attributes
            name="PlantE_ID"/>
        <attributes
            name="Name"/>
        <attributes
            name="Id"/>
        <attributes
            name="Wattage"
            value="25"/>
        <attributes
            name="Operational"
            value="true"/>
        <attributes
            name="Operational"
            value="True"/>
      </features>
    </instances>
    <instances
        name="BatteryDevice">
      <features
          name="Storage">
        <attributes
            name="Name"/>
        <attributes
            name="Id"/>
        <attributes
            name="Wattage"
            value="25"/>
        <attributes
            name="Operational"
            value="true"/>
        <attributes
            name="Properties"/>
        <attributes
            name="Capacity"
            value="30"/>
        <attributes
            name="Charging"
            value="true"/>
        <attributes
            name="ChargeT"
            value="Neutral"/>
        <attributes
            name="PlantE_ID"/>
      </features>
    </instances>
    <instances
        name="SmartMeter">
      <features
          name="SmartMeter">
        <attributes
            name="PlantE_ID"/>
        <attributes
            name="Tariff"/>
        <attributes
            name="Usage"/>
        <attributes
            name="Operational"
            value="True"/>
        <attributes
            name="Id"/>
      </features>
    </instances>
    <instances
        name="LegacyMeter">
      <features
          name="LegacyMeter">
        <attributes
            name="PlantE_ID"/>
        <attributes
            name="Operational"
            value="True"/>
        <attributes
            name="Id"/>
      </features>
    </instances>
  </resourceManager>
</sb.base:Manager>
