<?xml version="1.0" encoding="ASCII"?>
<sb.base:Manager
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sb.base="http://www.inf.ufg.br/mestrado/sb/base"
    xmlns:sb.base.common="http://www.inf.ufg.br/mestrado/sb/base/common"
    xsi:schemaLocation="http://www.inf.ufg.br/mestrado/sb/base base.ecore http://www.inf.ufg.br/mestrado/sb/base/common base.ecore#//common"
    name="mhb">
  <actions xsi:type="sb.base.common:MacroAction"
      name="TurnOnLoad"
      impl="mgridvm.mhb.macros.TurnOn">
    <parameters
        name="Id"/>
  </actions>
  <actions xsi:type="sb.base.common:MacroAction"
      name="TurnOffLoad"
      impl="mgridvm.mhb.macros.TurnOff">
    <parameters
        name="Id"/>
  </actions>
  <actions xsi:type="sb.base.common:MacroAction"
      name="FaultLoad"
      impl="mgridvm.mhb.macros.FaultLoad">
    <parameters
        name="load"/>
  </actions>
  <handlers
      signal="//@resourceManager/@iface/@signals.0">
    <action
        action="//@actions.2">
      <bindings
          parameter="//@actions.2/@parameters.0">
        <value
            xsi:type="sb.base.common:SignalSource"/>
      </bindings>
    </action>
  </handlers>
  <handlers
      signal="//@resourceManager/@iface/@provides.0">
    <action
        action="//@actions.0">
      <bindings
          parameter="//@actions.0/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@actions.1/@parameters.0"/>
      </bindings>
    </action>
  </handlers>
  <handlers
      signal="//@resourceManager/@iface/@provides.1">
    <action
        action="//@actions.1">
      <bindings
          parameter="//@actions.1/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@actions.1/@parameters.0"/>
      </bindings>
    </action>
  </handlers>
  <stateManager/>
  <resourceManager
      xsi:type="sb.base:InstanceResourceManager"
      Name="LoadController">
    <iface Name="LoadInterf">
      <provides
          name="turnOn">
        <parameters
            name="Id"/>
      </provides>
      <provides
          name="turnOff">
        <parameters
            name="Id"/>
      </provides>
      <signals
          name="fault"/>
    </iface>
    <instances
        name="Load1"
        impl="mgridvm.mhb.adapters.LoadControllerAdapter"
        iface="//@resourceManager/@iface">
      <features
          name="FeatureLoad1">
        <attributes
            name="AtributeFeatureLoad1"
            value="Load1Value"/>
      </features>
    </instances>
    <instances
        name="Load2"
        impl="mgridvm.mhb.adapters.LoadControllerAdapter"
        iface="//@resourceManager/@iface">
      <features
          name="Id">
        <attributes
            name="id"
            value="Load2"/>
      </features>
    </instances>
  </resourceManager>
</sb.base:Manager>
